
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <link href=" ~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href=" ~/Content/layui.css" rel="stylesheet" />
</head>
<body>
    <div style="margin: 10px 10px 10px 10px;">
        <form class="layui-form layui-form-pane" action="">

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.faimily" class="layui-form-label">家庭成員</label>
                    <div class="layui-input-inline">
                        <select name="interest" lay-filter="aihao">
                            <option value="">請選擇</option>
                            <option value="0">本人-代小武</option>
                            <option value="1">兒子-</option>
                            <option value="2">妻子-</option>
                            <option value="3">女兒</option>
                        </select>
                    </div>
                </div>
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.benefit" class="layui-form-label">福利*</label>
                    <div class="layui-input-inline">
                        <select name="interest" lay-filter="aihao">
                            <option value="">請選擇</option>
                            <option value="0">福利1</option>
                            <option value="1">福利2-</option>
                            <option value="2">福利3-</option>
                            <option value="3">福利4</option>
                        </select>
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.type" class="layui-form-label">發票類型</label>
                    <div class="layui-input-inline">
                        <select name="interest" lay-filter="aihao">
                            <option value="0" selected="">個人</option>
                            <option value="1">公司</option>
                        </select>
                    </div>
                </div>
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.no" class="layui-form-label">發票號</label>
                    <div class="layui-input-inline">
                        <input type="text" name="text" i18n="public.common.pleaseInput" i18n-only="placeholder" placeholder="请输入" autocomplete="off" class="layui-input" />
                    </div>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.service" class="layui-form-label">發票日</label>
                    <div class="layui-input-inline">
                        <input type="text" name="date" id="date" autocomplete="off" class="layui-input" onclick="layui.laydate({ elem: this })" />
                    </div>
                </div>
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.application" class="layui-form-label">申請日</label>
                    <div class="layui-input-inline">
                        <input type="text" name="date"1 id="date1" autocomplete="off" class="layui-input" onclick="layui.laydate({ elem: this })" />
                    </div>
                </div>
            </div>


            <div class="layui-form-item">
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.apply" class="layui-form-label">申請金額</label>
                    <div class="layui-input-inline">
                        <input type="text" name="username" lay-verify="required" i18n="public.common.pleaseInput" i18n-only="placeholder" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label i18n="Reimbursement.edit.invoice" class="layui-form-label">發票金額</label>
                    <div class="layui-input-inline">
                        <input type="text" name="username" lay-verify="required" i18n="public.common.pleaseInput" i18n-only="placeholder" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
            </div>

            <div class="layui-form-item layui-form-text">
                <label i18n="Reimbursement.edit.comments" class="layui-form-label">説明</label>
                <div class="layui-input-block">
                    <textarea i18n="public.common.pleaseInput" i18n-only="placeholder" placeholder="请输入" class="layui-textarea"></textarea>
                </div>
            </div>
            
        </form>
    </div>
</body>
</html>


<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/layui.js"></script>
<script src="~/Scripts/lay/modules/laydate.js"></script>
<script src="~/Scripts/lay/modules/form.js"></script>
<script src="~/Scripts/language/jquery.i18n.min.js"></script>
<script src="~/Scripts/language/laodlanguage.js"></script>
<script src="~/Scripts/jquery.cookie-1.4.1.min.js"></script>
<script type="text/javascript">
    $(function () {
        laodlanguage();
    });
</script>
