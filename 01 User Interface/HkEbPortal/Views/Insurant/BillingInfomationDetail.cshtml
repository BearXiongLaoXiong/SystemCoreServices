@using HkEbPortal.Models.EB_PORTAL
@model IEnumerable<SPEH_FMAD_ACCOUNT_DET_LIST_RESULT>

@{
    ViewBag.Title = "BillingInfomationDetail";
    Layout = null;
}
<link href="~/Scripts/css/layui.css" rel="stylesheet" />
<div class="container">
    <table class="layui-table" style="min-width:100px;max-width:1200px;min-height:100px;max-height:600px;">
        <thead>
            <tr>
                <th>日期</th>
                <th>类型</th>
                <th>分数</th>
                <th>当前分数</th>
                <th>备注</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td hidden>@item.FMAC_KY</td>
                    <td>@item.Transection_Date</td>
                    <td>@item.Transection_Type</td>
                    <td>@item.Points</td>
                    <td>@item.Current_Points</td>
                    <td>@item.Comment</td>
                </tr>
            }
        </tbody>
    </table>
</div>

