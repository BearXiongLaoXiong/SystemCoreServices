@using HkEbPortal.Models.EB_PORTAL
@model IEnumerable<SPEH_PLFM_DET_LIST_RESULT>
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <link href="../Content/style.css" rel="stylesheet" />
    <link href="../Scripts/css/layui.css" rel="stylesheet" />
    <script src="../Scripts/jquery-3.1.1.min.js"></script>
    <script src="../Scripts/layui.js"></script>
    <script src="../Scripts/common.js"></script>
    <script src="../Scripts/Policy/Detail.js"></script>
    <script src="../Scripts/language/jquery.i18n.min.js"></script>
    <script src="../Scripts/language/laodlanguage.js"></script>
    <script src="../Scripts/jquery.cookie-1.4.1.min.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>BenifitDetail</title>
</head>
<body style="padding:0;">
    <div class="container">
        <table class="layui-table" style="min-width:100px;max-width:1200px;min-height:100px;max-height:600px;">
            <thead>
                <tr>
                    <th i18n="Policy.index.Product">产品</th>
                    <th i18n="Policy.index.Plan">计划</th>
                    <th i18n="Policy.index.Points">积分</th>
                    <th i18n="Policy.index.Level">级别</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.PDCT_LONG_NAME</td>
                        <td>@item.PDPD_LONG_NAME</td>
                        <td>@item.PLME_AMT</td>
                        <td>@item.PDPD_LEVEL</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <script type="text/javascript">
        $(function () {
            laodlanguage();
        })
    </script>
</body>

</html>
