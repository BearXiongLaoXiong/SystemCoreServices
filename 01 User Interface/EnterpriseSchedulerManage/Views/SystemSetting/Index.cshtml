
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/custom/SynchronCustom.css" rel="stylesheet" />
<form class="layui-form" action="javascript:void()">
    <div class="layui-form-item" style="padding-bottom:-50px;">
        <div class="layui-inline">
            <label class="layui-form-label" style="width:86px;">同步内容</label>
            <div class="layui-input-inline">
                <select name="quiz">
                    <option value="-1">请选择同步内容</option>
                    <option value="HPHP">医院码</option>
                    <option value="DADA">诊断码</option>
                    <option value="SPSP">诊疗码</option>
                    <option value="SPCT">药品码</option>
                    <option value="DIDI">药品适应症</option>
                    <option value="DNDN">药品码别名</option>
                    <option value="SPON">诊疗码别名</option>
                    <option value="DODO">剂型码</option>
                    <option value="DOOT">剂型别名</option>
                    <option value="SHLS">诊疗目录</option>
                    <option value="STSP">服务诊疗匹配</option>
                    <option value="DCDC">药品分类</option>
                </select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:86px;">数据来源</label>
            <div class="layui-input-inline">
                <select name="modules" lay-verify="required" lay-search="">
                    <option value="">请选择数据来源</option>
                    <option value="1">核心-复兴配置</option>
                </select>
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label">码 值</label>
            <div class="layui-input-inline">
                <input type="text" name="email" lay-verify="email" autocomplete="off" class="layui-input">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:86px;">起始日期</label>
            <div class="layui-input-inline">
                <input type="text" id="dateStartId" placeholder="请选择起始日" class="layui-input" onclick="layui.laydate({elem: this, festival: true})">
            </div>
        </div>
        <div class="layui-inline">
            <label class="layui-form-label" style="width:86px;">截止日期</label>
            <div class="layui-input-inline">
                <input type="text" id="dateEndId" placeholder="请选择截止日" class="layui-input" onclick="layui.laydate({elem: this, festival: true})">
            </div>
            <button class="layui-btn" id="SynchronSearchId">查 询</button>
            <button class="layui-btn" id="SynchronId">同 步</button>
        </div>
    </div>
</form>
